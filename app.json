{
  "expo": {
    "name": "Robotik Mobile",
    "slug": "robotikm",
    "version": "1.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robotik.mobile"
    },
    "android": {
      "package": "com.robotik.mobile",
      "versionCode": 2,
      "useNextNotificationsApi": true,
      "permissions": [
        "POST_NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#9333ea"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#9333ea",
          "androidMode": "default",
          "androidCollapsedTitle": "Robotik notifikacije"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7c5bfec7-0a8a-49f6-89b4-7408977feb4f"
      }
    }
  }
}
