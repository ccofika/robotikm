{
  "expo": {
    "name": "Robotik Mobile",
    "slug": "robotikm",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robotik.mobile"
    },
    "android": {
      "package": "com.robotik.mobile",
      "useNextNotificationsApi": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#9333ea",
          "androidMode": "default",
          "androidCollapsedTitle": "Robotik notifikacije"
        }
      ]
    ]
  }
}
