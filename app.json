{
  "expo": {
    "name": "Robotik Mobile",
    "slug": "robotikm",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robotik.mobile",
      "icon": "./assets/icon.png"
    },
    "android": {
      "package": "com.robotik.mobile",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "POST_NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "REQUEST_IGNORE_BATTERY_OPTIMIZATIONS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#9333ea",
          "androidMode": "default",
          "androidCollapsedTitle": "Robotik notifikacije"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Aplikacija koristi vašu lokaciju za praćenje tehničara na mapi.",
          "locationAlwaysPermission": "Aplikacija koristi vašu lokaciju za praćenje tehničara na mapi.",
          "locationWhenInUsePermission": "Aplikacija koristi vašu lokaciju za praćenje tehničara na mapi.",
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      "expo-sqlite",
      "./plugins/withBackgroundLocationConfig"
    ],
    "extra": {
      "eas": {
        "projectId": "7c5bfec7-0a8a-49f6-89b4-7408977feb4f"
      }
    }
  }
}
